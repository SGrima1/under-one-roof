<%= simple_form_for([experience, item], remote: true, wrapper: :inline_form,
  html: { class: "form-inline" },
  wrapper_mappings: {
    boolean: :inline_boolean
  }) do |f| %>
  <%= f.input :name, label: "Add supply" %>
  <%= f.button :button, "    Add to supply list!    ", class: "btn btn-primary" %>
<% end %>
